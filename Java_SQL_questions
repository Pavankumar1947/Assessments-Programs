4. WAQTD employees earning more than Department Avg

	select e.emp_name, d.dept_name , e.salary
	from employee e
	JOIN department d ON d.dept_id = e.dept_id
	where e.salary > ( select AVG(e2.salary)
					 from employee e2
					 where e2.dept_id = e1.dept_id);